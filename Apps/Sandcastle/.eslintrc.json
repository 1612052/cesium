{
    "extends": "../../.eslintrc.json",
    "env": {
        "amd": true
    },
    "globals": {
        "JSON": true,
        "require": true,
        "console": true,
        "Sandcastle": true,
        "Cesium": true
    },
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script"
    },
    "rules": {
        "no-alert": ["off"],
        "no-implicit-globals": "off",
        "no-unused-vars": ["off"],
        "quotes": "off"
    }
}
